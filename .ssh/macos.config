Host workstation.benjamindossantos.dev
  HostName workstation.benjamindossantos.dev
  User benjamin

Match host workstation.benjamindossantos.dev exec "/opt/homebrew/bin/cloudflared access ssh-gen --hostname %h"
  ProxyCommand /opt/homebrew/bin/cloudflared access ssh --hostname %h
  IdentityFile ~/.cloudflared/%h-cf_key
  CertificateFile ~/.cloudflared/%h-cf_key-cert.pub
